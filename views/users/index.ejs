<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User List</title>
</head>
<body>

    <table id="list">
        <thead>
          <tr>
            <th>ID</th>
            <th>Username</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% users.forEach(function(u) { %>
            <tr>
              <td><%= u.id %></td>
              <td><%= u.username %></td>
              <td><a href="/users/<%= u._id %>">DETAILS</a></td>
            </tr>
            <% }); %>
          </tbody>
        </table>
    
</body>
</html>